  </CellMethods>
  <BaseMethods>
    <smartDestroy></smartDestroy>
  </BaseMethods>
  <TempProperties>
    <_components></_components>
    <_protectionZone></_protectionZone>
    <_entities></_entities>
  </TempProperties>
</ProtectionZone.def>
