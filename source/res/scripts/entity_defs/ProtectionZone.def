<ProtectionZone.def>
  <IsManualAoI>	true	</IsManualAoI>
  <Properties>
    <arena>
      <Type>	MAILBOX	</Type>
      <Flags>	BASE	</Flags>
    </arena>
    <arenaBase>
      <Type>	MAILBOX	</Type>
      <Flags>	CELL_PRIVATE	</Flags>
    </arenaBase>
    <zoneID>
      <Type>	UINT8	</Type>
      <Flags>	ALL_CLIENTS	</Flags>
      <Default>	1	</Default>
      <Editable>	true	</Editable>
    </zoneID>
    <lengthX>
      <Type>	FLOAT32	</Type>
      <Flags>	ALL_CLIENTS	</Flags>
      <Default>	300.0	</Default>
      <Editable>	true	</Editable>
    </lengthX>
    <lengthZ>
      <Type>	FLOAT32	</Type>
      <Flags>	ALL_CLIENTS	</Flags>
      <Default>	500.0	</Default>
      <Editable>	true	</Editable>
    </lengthZ>
    <team>
      <Type>	UINT8	</Type>
      <Flags>	ALL_CLIENTS	</Flags>
      <Default>	0	</Default>
      <Editable>	true	</Editable>
    </team>
    <maxStayTime>
      <Type>	FLOAT32	</Type>
      <Flags>	CELL_PUBLIC	</Flags>
      <Default>	5.0	</Default>
      <Editable>	true	</Editable>
    </maxStayTime>
    <numberOfTurrets>
      <Type>	UINT8	</Type>
      <Flags>	CELL_PRIVATE	</Flags>
      <Default>	2	</Default>
      <Editable>	true	</Editable>
