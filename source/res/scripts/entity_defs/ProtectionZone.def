    <minTurretShootInterval>
      <Type>	FLOAT32	</Type>
      <Flags>	CELL_PRIVATE	</Flags>
      <Default>	2.0	</Default>
      <Editable>	true	</Editable>
    </minTurretShootInterval>
    <minShootingTime>
      <Type>	FLOAT32	</Type>
      <Flags>	CELL_PRIVATE	</Flags>
      <Default>	15.0	</Default>
      <Editable>	true	</Editable>
    </minShootingTime>
    <shotDuration>
      <Type>	FLOAT32	</Type>
      <Flags>	CELL_PRIVATE	</Flags>
      <Default>	1.0	</Default>
      <Editable>	true	</Editable>
    </shotDuration>
    <shotRadius>
      <Type>	FLOAT32	</Type>
      <Flags>	CELL_PRIVATE	</Flags>
      <Default>	5.0	</Default>
      <Editable>	true	</Editable>
    </shotRadius>
    <shotShellNation>
      <Type>	STRING	</Type>
