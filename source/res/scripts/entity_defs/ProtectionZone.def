    <mainDirection>
      <Type>	UINT8	</Type>
      <Flags>	CELL_PRIVATE	</Flags>
      <Default>	0	</Default>
      <Editable>	true	</Editable>
    </mainDirection>
    <isActive>
      <Type>	BOOL	</Type>
      <Flags>	ALL_CLIENTS	</Flags>
      <Default>	False	</Default>
    </isActive>
    <cp>
      <Type>	PYTHON	</Type>
      <Flags>	CELL_PRIVATE	</Flags>
    </cp>
  </Properties>
  <CellMethods>
    <start></start>
    <reset></reset>
    <stop></stop>
    <harm_receiveAttackResults>
      <Arg>	ATTACK_RESULTS	</Arg>
