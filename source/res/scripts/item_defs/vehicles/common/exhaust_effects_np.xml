<exhaust_effects_np.xml>
  <constants>
    <constantsGroup>
      <name>	diesel_small	</name>
      <constant>
        <name>	_fileDescriptor	</name>
        <value>	small_diesel	</value>
      </constant>
    </constantsGroup>
    <constantsGroup>
      <name>	diesel_medium	</name>
      <constant>
        <name>	_fileDescriptor	</name>
        <value>	medium_diesel	</value>
      </constant>
    </constantsGroup>
    <constantsGroup>
      <name>	diesel_large	</name>
      <constant>
        <name>	_fileDescriptor	</name>
        <value>	large_diesel	</value>
      </constant>
    </constantsGroup>
    <constantsGroup>
      <name>	gas_small	</name>
      <constant>
        <name>	_fileDescriptor	</name>
        <value>	small_gas	</value>
      </constant>
    </constantsGroup>
    <constantsGroup>
      <name>	gas_medium	</name>
      <constant>
        <name>	_fileDescriptor	</name>
        <value>	medium_gas	</value>
      </constant>
    </constantsGroup>
    <constantsGroup>
      <name>	gas_large	</name>
      <constant>
        <name>	_fileDescriptor	</name>
        <value>	large_gas	</value>
      </constant>
    </constantsGroup>
  </constants>
  <templates>
    <template>
      <name>	GearUp	</name>
      <selector>
        <type>	discrete	</type>
        <variable>	_gearUp	</variable>
        <selector>
          <key>	1	</key>
          <type>	effect	</type>
          <ttl>	0.7	</ttl>
          <name>	particles/Tank/exhaust/{0}/gear_up.xml	</name>
        </selector>
      </selector>
    </template>
    <template>
      <name>	EngineStart	</name>
      <selector>
        <type>	discrete	</type>
        <variable>	_engineStart	</variable>
        <selector>
          <key>	1	</key>
          <type>	effect	</type>
          <ttl>	5.2	</ttl>
          <name>	particles/Tank/exhaust/{0}/start.xml	</name>
        </selector>
      </selector>
    </template>
    <template>
      <name>	Exhaust	</name>
      <selector>
        <type>	range	</type>
        <variable>	_engine_load	</variable>
        <selector>
          <key>	1	</key>
          <type>	effect	</type>
          <name>	particles/Tank/exhaust/{0}/idle.xml	</name>
        </selector>
        <selector>
          <key>	2	</key>
          <type>	range	</type>
          <variable>	_physicLoad	</variable>
          <selector>
            <key>	0	</key>
            <type>	range	</type>
            <variable>	_rpm	</variable>
            <selector>
              <key>	0.00	</key>
              <type>	effect	</type>
              <name>	particles/Tank/exhaust/{0}/rpm2.xml	</name>
            </selector>
            <selector>
              <key>	0.03	</key>
              <type>	effect	</type>
              <name>	particles/Tank/exhaust/{0}/rpm1.xml	</name>
            </selector>
            <selector>
              <key>	0.20	</key>
              <type>	effect	</type>
              <name>	particles/Tank/exhaust/{0}/idle.xml	</name>
            </selector>
            <selector>
              <key>	1.05	</key>
              <type>	effect	</type>
              <name>	particles/Tank/exhaust/{0}/rpm4.xml	</name>
            </selector>
          </selector>
          <selector>
            <key>	0.45	</key>
            <type>	range	</type>
            <variable>	_rpm	</variable>
            <selector>
              <key>	0.00	</key>
              <type>	effect	</type>
              <name>	particles/Tank/exhaust/{0}/rpm3.xml	</name>
            </selector>
            <selector>
              <key>	0.10	</key>
              <type>	effect	</type>
              <name>	particles/Tank/exhaust/{0}/rpm2.xml	</name>
            </selector>
            <selector>
              <key>	0.40	</key>
              <type>	effect	</type>
              <name>	particles/Tank/exhaust/{0}/rpm1.xml	</name>
            </selector>
            <selector>
              <key>	1.05	</key>
              <type>	effect	</type>
              <name>	particles/Tank/exhaust/{0}/rpm4.xml	</name>
            </selector>
          </selector>
          <selector>
            <key>	0.55	</key>
            <type>	range	</type>
            <variable>	_rpm	</variable>
            <selector>
              <key>	0.00	</key>
              <type>	effect	</type>
              <name>	particles/Tank/exhaust/{0}/rpm4.xml	</name>
            </selector>
            <selector>
              <key>	0.30	</key>
              <type>	effect	</type>
              <name>	particles/Tank/exhaust/{0}/rpm3.xml	</name>
            </selector>
            <selector>
              <key>	0.80	</key>
              <type>	effect	</type>
              <name>	particles/Tank/exhaust/{0}/rpm2.xml	</name>
            </selector>
            <selector>
              <key>	1.05	</key>
              <type>	effect	</type>
              <name>	particles/Tank/exhaust/{0}/rpm4.xml	</name>
            </selector>
          </selector>
          <selector>
            <key>	0.95	</key>
            <type>	range	</type>
            <variable>	_rpm	</variable>
            <selector>
              <key>	0.00	</key>
              <type>	effect	</type>
              <name>	particles/Tank/exhaust/{0}/rpm4.xml	</name>
            </selector>
          </selector>
        </selector>
      </selector>
    </template>
    <template>
      <name>	Exhaust1	</name>
      <selector>
        <type>	union	</type>
        <selector>
          <type>	template	</type>
          <name>	Exhaust	</name>
          <parameters>	_engine_load:engineLoad;
				_physicLoad:physicLoad;
				_rpm:RPM	</parameters>
        </selector>
        <selector>
          <type>	template	</type>
          <name>	GearUp	</name>
          <parameters>	_gearUp:gearUp	</parameters>
        </selector>
        <selector>
          <type>	template	</type>
          <name>	EngineStart	</name>
          <parameters>	_engineStart:engineStart	</parameters>
        </selector>
      </selector>
    </template>
  </templates>
  <effects>
    <default>
      <selector>
        <type>	template	</type>
        <name>	Exhaust1	</name>
        <constantsGroup>	gas_medium	</constantsGroup>
      </selector>
    </default>
    <diesel_small>
      <selector>
        <type>	template	</type>
        <name>	Exhaust1	</name>
        <constantsGroup>	diesel_small	</constantsGroup>
