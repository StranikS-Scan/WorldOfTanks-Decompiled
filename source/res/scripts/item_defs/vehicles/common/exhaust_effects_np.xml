    <diesel_large>
      <selector>
        <type>	template	</type>
        <name>	Exhaust1	</name>
        <constantsGroup>	diesel_large	</constantsGroup>
      </selector>
    </diesel_large>
    <gas_small>
      <selector>
        <type>	template	</type>
        <name>	Exhaust1	</name>
        <constantsGroup>	gas_small	</constantsGroup>
      </selector>
    </gas_small>
    <gas_medium>
      <selector>
        <type>	template	</type>
        <name>	Exhaust1	</name>
        <constantsGroup>	gas_medium	</constantsGroup>
      </selector>
    </gas_medium>
    <gas_large>
      <selector>
        <type>	template	</type>
        <name>	Exhaust1	</name>
        <constantsGroup>	gas_large	</constantsGroup>
      </selector>
    </gas_large>
  </effects>
</exhaust_effects_np.xml>
