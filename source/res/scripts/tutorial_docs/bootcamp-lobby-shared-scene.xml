<bootcamp-lobby-shared-scene.xml>
  <post-effects>
    <effects-group>
      <condition>
        <effect-not-triggered>	CheckpointController	</effect-not-triggered>
      </condition>
      <effects>
        <invoke-gui-cmd>	OverrideHeaderMenuButtons	</invoke-gui-cmd>
        <set-visible>	AmmunitionPanel	  <val>	False	</val>
        </set-visible>
        <set-visible>	HangarEquipmentSlots	  <val>	False	</val>
        </set-visible>
        <set-visible>	HangarOptionalDeviceSlots	  <val>	False	</val>
        </set-visible>
        <set-visible>	AmmunitionSetupOptionalDeviceSlots	  <val>	False	</val>
        </set-visible>
        <set-visible>	MessengerBar	  <val>	False	</val>
        </set-visible>
        <set-visible>	QuestsControl	  <val>	False	</val>
        </set-visible>
        <set-visible>	VehicleResearchPanel	  <val>	False	</val>
        </set-visible>
        <set-visible>	VehicleResearchPanelBG	  <val>	False	</val>
        </set-visible>
        <set-visible>	VehicleResearchButton	  <val>	False	</val>
        </set-visible>
        <set-visible>	HangarParams	  <val>	False	</val>
        </set-visible>
        <set-visible>	RepairBtn	  <val>	False	</val>
        </set-visible>
        <set-visible>	TuningBtn	  <val>	False	</val>
        </set-visible>
        <set-visible>	VehicleStateMessage	  <val>	False	</val>
        </set-visible>
        <set-visible>	TankCarouselContainer	  <val>	False	</val>
        </set-visible>
        <set-visible>	TankCarouselContainerBG	  <val>	False	</val>
        </set-visible>
        <set-visible>	PersonalCaseDropSkillsButton	  <val>	False	</val>
        </set-visible>
        <set-allowed-to-fight>
          <value>	False	</value>
        </set-allowed-to-fight>
        <run-trigger>	CurrentVehicleChangeTrigger	</run-trigger>
        <run-trigger>	ItemsCacheSyncTrigger	</run-trigger>
        <restore-checkpoint></restore-checkpoint>
        <run-trigger>	CheckpointController	</run-trigger>
      </effects>
    </effects-group>
    <invoke-gui-cmd>	HideQuests	  <condition>
        <effect-not-triggered>	ShowQuests	</effect-not-triggered>
      </condition>
    </invoke-gui-cmd>
  </post-effects>
  <runtime-effects></runtime-effects>
  <gui-items></gui-items>
</bootcamp-lobby-shared-scene.xml>
