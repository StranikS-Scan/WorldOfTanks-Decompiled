<quests-shared-scene.xml>
  <runtime-effects>
    <select-in-hangar>	SelectedVehicleCriteria	  <condition>
        <inactive>	PveChainShown	</inactive>
        <active>	DefaultSandboxQueue	</active>
        <active>	InSandbox	</active>
        <active>	NoPveVehicleInInventory	</active>
        <effect-not-triggered>	SelectedVehicleCriteria	</effect-not-triggered>
      </condition>
    </select-in-hangar>
    <switch-to-random>
      <condition>
        <effect-not-triggered>	BattleSelectorHint	</effect-not-triggered>
        <effect-triggered>	SelectedVehicleCriteria	</effect-triggered>
      </condition>
    </switch-to-random>
    <show-hint>	BattleSelectorHint	  <condition>
        <active>	BattleSelectorHintOnScene	</active>
        <effect-triggered>	SelectedVehicleCriteria	</effect-triggered>
        <effect-not-triggered>	BattleSelectorHint	</effect-not-triggered>
      </condition>
    </show-hint>
    <show-hint>	TankAcademyButtonHint	  <condition>
        <active>	TankAcademyButtonOnScene	</active>
        <active>	PlayerHasTankmanDiscount	</active>
      </condition>
    </show-hint>
    <show-hint>	QuestsControlHint	  <condition>
        <active>	QuestsControlOnScene	</active>
        <active>	ShowQuestsControlHint	</active>
        <inactive>	LobbyMenuOnScene	</inactive>
      </condition>
    </show-hint>
    <show-hint>	ResearchHelpButtonHint	  <condition>
        <active>	ShowResearchHint	</active>
        <active>	ResearchHelpButtonOnScene	</active>
      </condition>
    </show-hint>
    <show-hint>	TechTreeHelpButtonHint	  <condition>
        <active>	ShowTechTreeHint	</active>
        <active>	TechTreeHelpButtonOnScene	</active>
      </condition>
    </show-hint>
    <show-hint>	PersonalCaseHelpButtonHint	  <condition>
        <active>	ShowPersonalCaseHint	</active>
        <active>	PersonalCaseHelpButtonOnScene	</active>
      </condition>
    </show-hint>
    <show-hint>	TechnicalMaintenanceHelpButtonHint	  <condition>
        <active>	ShowTechnicalMaintenanceHint	</active>
        <active>	TechnicalMaintenanceHelpButtonOnScene	</active>
      </condition>
    </show-hint>
    <show-hint>	CustomizationHelpButtonHint	  <condition>
        <active>	ShowCustomizationHint	</active>
        <active>	CustomizationHelpButtonOnScene	</active>
      </condition>
    </show-hint>
    <show-award-window>	TutorialAwardWindow5	  <condition>
        <global-active>	SlotDiscountUsed	</global-active>
        <bonus-not-received>	TutorialQuest_5	</bonus-not-received>
      </condition>
    </show-award-window>
    <request-bonus>	TutorialQuest_5	  <condition>
        <global-active>	SlotDiscountUsed	</global-active>
        <bonus-not-received>	TutorialQuest_5	</bonus-not-received>
      </condition>
    </request-bonus>
  </runtime-effects>
  <gui-items>
    <gui-item>	LobbyMenu	  <on-scene-effects>
        <activate>	LobbyMenuOnScene	</activate>
      </on-scene-effects>
      <not-on-scene-effects>
        <inactivate>	LobbyMenuOnScene	</inactivate>
