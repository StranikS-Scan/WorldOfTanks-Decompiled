<tutorial-battle-quests.xml>
  <default-scene>	DefaultScene	</default-scene>
  <initial-scene>	Battle	</initial-scene>
  <scenes>
    <scene>	Battle	  <post-effects>
        <run-trigger>	MedkitUseTrigger	</run-trigger>
        <run-trigger>	RepairKitUseTrigger	</run-trigger>
        <run-trigger>	FireExtinguisherInstalledTrigger	</run-trigger>
      </post-effects>
      <runtime-effects></runtime-effects>
      <gui-items></gui-items>
    </scene>
  </scenes>
  <has-id>
    <tutorial-setting>	MedKitUsedSetting	  <setting-name>	medKitUsed	</setting-name>
      <setting-value>	True	</setting-value>
    </tutorial-setting>
    <tutorial-setting>	RepairKitUsedSetting	  <setting-name>	repairKitUsed	</setting-name>
      <setting-value>	True	</setting-value>
    </tutorial-setting>
    <tutorial-setting>	FireExtinguisherWasInBattleSetting	  <setting-name>	fireExtinguisherUsed	</setting-name>
      <setting-value>	True	</setting-value>
    </tutorial-setting>
  </has-id>
  <vars>
    <var-set>	MedKitsConditionVar	  <asDict>
        <items>
          <asList>
            <asInt>	1019	</asInt>
            <asInt>	763	</asInt>
          </asList>
        </items>
      </asDict>
    </var-set>
    <var-set>	RepairKitsConditionVar	  <asDict>
        <items>
          <asList>
            <asInt>	1531	</asInt>
            <asInt>	1275	</asInt>
          </asList>
        </items>
      </asDict>
    </var-set>
    <var-set>	FireExtinguishersConditionVar	  <asDict>
        <items>
          <asList>
            <asInt>	507	</asInt>
            <asInt>	251	</asInt>
          </asList>
        </items>
      </asDict>
    </var-set>
    <var-set>	FireExtinguisherInstalledVar	  <asString>	fireExtinguisherInstalled	</asString>
    </var-set>
  </vars>
  <triggers>
    <trigger>	MedkitUseTrigger	  <type>	useItem	</type>
      <validate-var>	MedKitsConditionVar	</validate-var>
      <on-effects>
        <save-setting>	MedKitUsedSetting	</save-setting>
      </on-effects>
      <off-effects></off-effects>
    </trigger>
    <trigger>	RepairKitUseTrigger	  <type>	useItem	</type>
      <validate-var>	RepairKitsConditionVar	</validate-var>
      <on-effects>
        <save-setting>	RepairKitUsedSetting	</save-setting>
      </on-effects>
      <off-effects></off-effects>
    </trigger>
    <trigger>	FireExtinguisherInstalledTrigger	  <type>	installItem	</type>
      <validate-var>	FireExtinguishersConditionVar	</validate-var>
      <on-effects>
        <save-setting>	FireExtinguisherWasInBattleSetting	</save-setting>
      </on-effects>
      <off-effects></off-effects>
    </trigger>
  </triggers>
</tutorial-battle-quests.xml>
