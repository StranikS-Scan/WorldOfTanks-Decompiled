<newOnboarding_holdPosition_danceForthAndBack.xml>
  <xmlns:xsd>	http://www.w3.org/2001/XMLSchema	</xmlns:xsd>
  <xmlns:xsi>	http://www.w3.org/2001/XMLSchema-instance	</xmlns:xsi>
  <xsi:schemaLocation>	WoT.AI/BTree server_xml/ai/schemas/behaviortree/vehiclebot_bthost.xsd	</xsi:schemaLocation>
  <xmlns>	WoT.AI/BTree	</xmlns>
  <Tree>
    <rootType>	Sequence	</rootType>
    <host>	vehiclebot_bthost	</host>
    <Node>
      <xsi:type>	Leaf	</xsi:type>
      <function>	saveCurrentPosition	</function>
    </Node>
    <Node>
      <xsi:type>	DecoratorRunning	</xsi:type>
      <name>	Dance if there is a target or stay	</name>
      <Node>
        <xsi:type>	DecoratorFail	</xsi:type>
        <Node>
          <xsi:type>	Sequence	</xsi:type>
          <name></name>
          <Node>
            <xsi:type>	Leaf	</xsi:type>
            <function>	checkTarget	</function>
            <param>	anyDetectedTarget	</param>
          </Node>
          <Node>
            <xsi:type>	RandomChoice	</xsi:type>
            <name>	Move forward	</name>
            <Node>
              <xsi:type>	Leaf	</xsi:type>
              <probability>	0.5	</probability>
              <function>	move	</function>
              <param>	6MeterForward	</param>
            </Node>
            <Node>
              <xsi:type>	Leaf	</xsi:type>
              <probability>	0.5	</probability>
              <function>	move	</function>
              <param>	7MeterForward	</param>
            </Node>
          </Node>
          <Node>
            <xsi:type>	Leaf	</xsi:type>
            <function>	waitUntilTargetReached	</function>
          </Node>
          <Node>
            <xsi:type>	RandomChoice	</xsi:type>
            <name>	Probably wait for some time	</name>
            <Node>
              <xsi:type>	Leaf	</xsi:type>
              <probability>	0.3	</probability>
              <function>	PASS	</function>
            </Node>
            <Node>
              <xsi:type>	Sequence	</xsi:type>
              <probability>	0.7	</probability>
              <name>	Wait for some time	</name>
              <Node>
                <xsi:type>	RandomChoice	</xsi:type>
                <name>	Set random timer	</name>
                <Node>
                  <xsi:type>	Leaf	</xsi:type>
                  <probability>	0.8	</probability>
                  <function>	setRandomTimer	</function>
                  <param>	about1Second	</param>
                </Node>
                <Node>
                  <xsi:type>	Leaf	</xsi:type>
                  <probability>	0.2	</probability>
                  <function>	setRandomTimer	</function>
                  <param>	about2-3Seconds	</param>
                </Node>
              </Node>
              <Node>
                <xsi:type>	Leaf	</xsi:type>
                <function>	waitForTimerExpires	</function>
                <param>	about1Second	</param>
              </Node>
            </Node>
          </Node>
          <Node>
            <xsi:type>	Leaf	</xsi:type>
            <function>	move	</function>
            <param>	moveBackToSavedPosition	</param>
          </Node>
          <Node>
            <xsi:type>	Leaf	</xsi:type>
            <function>	waitUntilTargetReached	</function>
          </Node>
          <Node>
            <xsi:type>	RandomChoice	</xsi:type>
            <name>	Probably wait for some time	</name>
            <Node>
              <xsi:type>	Leaf	</xsi:type>
              <probability>	0.3	</probability>
              <function>	PASS	</function>
            </Node>
            <Node>
              <xsi:type>	Sequence	</xsi:type>
              <probability>	0.7	</probability>
              <name>	Wait for some time	</name>
              <Node>
                <xsi:type>	RandomChoice	</xsi:type>
                <name>	Set random timer	</name>
                <Node>
                  <xsi:type>	Leaf	</xsi:type>
                  <probability>	0.6	</probability>
                  <function>	setRandomTimer	</function>
                  <param>	about1Second	</param>
                </Node>
                <Node>
                  <xsi:type>	Leaf	</xsi:type>
                  <probability>	0.4	</probability>
                  <function>	setRandomTimer	</function>
                  <param>	about2-5Seconds	</param>
                </Node>
              </Node>
              <Node>
                <xsi:type>	Leaf	</xsi:type>
                <function>	waitForTimerExpires	</function>
                <param>	about1Second	</param>
              </Node>
            </Node>
          </Node>
          <Node>
            <xsi:type>	Leaf	</xsi:type>
            <function>	face	</function>
            <param>	faceSavedDirection	</param>
          </Node>
          <Node>
            <xsi:type>	Leaf	</xsi:type>
            <function>	waitUntilTargetFaced	</function>
          </Node>
        </Node>
      </Node>
    </Node>
  </Tree>
  <Parameter>
    <xsi:type>	DirectMoveParam	</xsi:type>
    <id>	6MeterForward	</id>
    <distance>
      <value>	6	</value>
    </distance>
    <optionsMask>
      <value>	PreferForward	</value>
    </optionsMask>
  </Parameter>
  <Parameter>
    <xsi:type>	SetRandomTimerParam	</xsi:type>
    <id>	about1Second	</id>
    <timerName>
      <value>	wait	</value>
    </timerName>
    <minTime>
      <value>	0.4	</value>
    </minTime>
    <maxTime>
      <value>	0.8	</value>
    </maxTime>
  </Parameter>
  <Parameter>
    <xsi:type>	SetRandomTimerParam	</xsi:type>
    <id>	about2-5Seconds	</id>
    <timerName>
      <value>	wait	</value>
    </timerName>
    <minTime>
      <value>	1.5	</value>
    </minTime>
    <maxTime>
      <value>	4.5	</value>
    </maxTime>
  </Parameter>
  <Parameter>
    <xsi:type>	DirectMoveParam	</xsi:type>
    <id>	7MeterForward	</id>
    <distance>
      <value>	7	</value>
    </distance>
    <optionsMask>
      <value>	PreferForward	</value>
    </optionsMask>
  </Parameter>
  <Parameter>
    <xsi:type>	MoveParam	</xsi:type>
    <id>	moveBackToSavedPosition	</id>
    <positionType>
      <value>	SavedPosition	</value>
    </positionType>
    <arriveDistance>
      <value>	2	</value>
    </arriveDistance>
    <optionsMask>
      <value>	PreferBackward	</value>
    </optionsMask>
    <optionsMask>
      <value>	NoPathFinding	</value>
    </optionsMask>
    <useMovementLimitsForPathfinding>
      <value>	false	</value>
    </useMovementLimitsForPathfinding>
  </Parameter>
  <Parameter>
    <xsi:type>	SetRandomTimerParam	</xsi:type>
    <id>	about2-3Seconds	</id>
    <timerName>
      <value>	wait	</value>
    </timerName>
    <minTime>
      <value>	1.5	</value>
    </minTime>
    <maxTime>
      <value>	2.5	</value>
    </maxTime>
  </Parameter>
  <Parameter>
    <xsi:type>	FaceParam	</xsi:type>
    <id>	faceSavedDirection	</id>
    <facingType>
      <value>	SavedDirection	</value>
    </facingType>
  </Parameter>
  <Parameter>
    <xsi:type>	checkTargetParam	</xsi:type>
    <id>	anyDetectedTarget	</id>
    <directlyDetectedOnly>
      <value>	false	</value>
    </directlyDetectedOnly>
    <undetectedTimeout>
      <value>	0	</value>
    </undetectedTimeout>
    <unexposedTimeout>
      <value>	900	</value>
    </unexposedTimeout>
    <maxDistance>
      <value>	565	</value>
    </maxDistance>
    <hitChance>
      <value>	0	</value>
    </hitChance>
    <hitChanceCheckMode>
      <value>	DontWait	</value>
    </hitChanceCheckMode>
  </Parameter>
</newOnboarding_holdPosition_danceForthAndBack.xml>
